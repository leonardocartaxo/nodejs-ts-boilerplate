{
  "name": "nodejs-ts-boilerplate",
  "version": "0.1.0",
  "description": "Node.js boilerplate project written in typescript that implements DDD, SOLID, clean architecture...",
  "scripts": {
    "build:serverless": "serverless package",
    "tsc-check": "tsc --project tsconfig.json --noEmit",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint-fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "start:local:serverless": "serverless offline",
    "deploy:serverless": "serverless deploy",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonardocartaxo/nodejs-ts-boilerplate.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "rest",
    "solid",
    "clean-architeture",
    "solid"
  ],
  "author": "Leonardo Cartaxo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leonardocartaxo/nodejs-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/leonardocartaxo/nodejs-ts-boilerplate#readme",
  "devDependencies": {
    "@typegoose/typegoose": "^9.4.0",
    "@types/aws-lambda": "^8.10.89",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "aws-lambda": "^1.0.7",
    "dotenv": "^10.0.0",
    "jest": "^27.4.7",
    "mongodb-memory-server": "^8.1.0",
    "serverless-dotenv-plugin": "^3.12.2",
    "serverless-offline": "^8.3.1",
    "serverless-plugin-typescript": "^2.1.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "class-transformer": "^0.4.0",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.13.2",
    "http-status-codes": "^2.2.0",
    "mongoose": "^6.1.5"
  }
}
